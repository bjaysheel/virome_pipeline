#!/bin/sh

if [ -z "$OUTPUT_DIR" ]
then
    OUTPUT_DIR=.
export OUTPUT_DIR
fi
if [ -z "$WRITER_CONF" ]
then
    WRITER_CONF=Chado:BulkSybase:SYBTIGR
export WRITER_CONF
fi
if [ -z "$DBCACHE" ]
then
    DBCACHE=1
export DBCACHE
fi
if [ -z "$COMMIT_ORDER" ]
then
    COMMIT_ORDER=db,dbxref,organism_dbxref,feature,featureloc,feature_cvterm,feature_dbxref,featureprop,feature_relationship
export COMMIT_ORDER
fi
if [ -z "$PRISM" ]
then
    PRISM=Euk:Sybase:SYBTIGR
export PRISM
fi
if [ -z "$ID_REPOSITORY" ]
then
    ID_REPOSITORY=.
export ID_REPOSITORY
fi


exec /usr/bin/perl -I /diag/projects/virome/package_virome/lib/perl5 /diag/projects/virome/package_virome/bin/euktigr2chado.pl "$@"    

