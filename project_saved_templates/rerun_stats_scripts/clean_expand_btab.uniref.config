[interface]
classification = virome

[parameter]
;; lookup dir location
$;LOOKUP_DIR$;=/diag/projects/virome/lookup
;; environment where script is ran igs, dbi, ageek
$;LOCATION$;=diag5

[input]
;; input must be btab blast output file/list/dir
$;INPUT_FILE_LIST$;=/diag/projects/virome/output_repository/btab2viromebtab/C88CA8548A32_uniref/btab2viromebtab.btab.list
$;INPUT_FILE$;=
$;INPUT_DIRECTORY$;=
;; the following is only used when iterating over an INPUT_DIRECTORY
$;INPUT_EXTENSION$;=fsa

[output]
$;OUTPUT_TOKEN$;=uniref
$;OUTPUT_DIRECTORY$;=$;REPOSITORY_ROOT$;/output_repository/$;COMPONENT_NAME$;/$;PIPELINEID$;_$;OUTPUT_TOKEN$;
$;CLEAN_OUTPUT_LIST$;=$;OUTPUT_DIRECTORY$;/$;COMPONENT_NAME$;.modified.list

[component]
$;COMPONENT_NAME$;=clean_expand_btab
$;DESCRIPTION$;=Remove extra tabs, and if subject DB is UNIREF100P then expand results.
$;WORKFLOW_REPOSITORY$;=$;REPOSITORY_ROOT$;/workflow/runtime/$;COMPONENT_NAME$;/$;PIPELINEID$;_$;OUTPUT_TOKEN$;
$;PIPELINE_TOKEN$;=unnamed

;The version,revision,tag here is set by an interpolated CVS tag
$;VERSION$;=1.0
$;RELEASE_TAG$;=$Name$
$;REVISION$;=$Revision: 1 $

$;TEMPLATE_XML$;=$;DOCS_DIR$;/$;COMPONENT_NAME$;.xml
$;ITERATOR1$;=i1
$;ITERATOR1_XML$;=$;DOCS_DIR$;/$;COMPONENT_NAME$;.$;ITERATOR1$;.xml

;Distributed options
$;GROUP_COUNT$;=150
$;NODISTRIB$;=0

;the following keys are replaced at runtime by the invocation script
$;COMPONENT_CONFIG$;=
$;COMPONENT_XML$;=
$;PIPELINE_XML$;=
$;PIPELINEID$;=

[include]
$;PROJECT_CONFIG$;=/diag/projects/virome/workflow/project.config
