<?xml version="1.0" encoding="UTF-8"?>

<commandSetRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation='commandSet.xsd'>
<commandSet type="serial">
	<name>ADD NO-HITS</name>
	<!--Processing-->
	<command>
		<type>RunUnixCommand</type>
		<name>add dummy NOHIT values to blastp</name>
		<state>incomplete</state>
		<executable>$;BIN_DIR$;/db-load-nohit</executable>

		<param>
			<key>--server</key>
			<value>$;SERVER_NAME$;</value>
		</param>
		<param>
			<key>--library</key>
			<value>$;LIBRARY_ID$;</value>
		</param>
		<param>
			<key>--env</key>
			<value>$;LOCATION$;</value>
		</param>
		<param>
			<key>--input</key>
			<value>$;I_FILE_PATH$;</value>
		</param>
		<param>
			<key>--outdir</key>
			<value>$;OUTPUT_DIRECTORY$;</value>
		</param>
		<param>
			<key>--pipelineid</key>
			<value>$;PIPELINEID$;</value>
		</param>
		<param>
			<key>stdout</key>
			<value>$;TMP_DIR$;/db-load-nohit.stdout</value>
		</param>
		<param>
			<key>stderr</key>
			<value>$;TMP_DIR$;/db-load-hit.stderr</value>
		</param>
	</command>
</commandSet>
</commandSetRoot>

