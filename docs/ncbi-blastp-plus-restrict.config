$;EXPECT$; = 1e-0
;-word_size Word size for alignment default = 3
$;WORDSIZE$; = 3
;-max_target_seqs Number of hits per query to record (default=500)
$;MAX_TARGET_SEQS$; = 500
;-num_threads Number of threads to use default = 1
$;NUM_THREADS$; = 1
;-dbsize Effective length of the database (size of DB in AAs
$;DBSIZE$; = 


[input]
$;INPUT_FILE_LIST$; = 
$;INPUT_FILE$; = 
$;INPUT_DIRECTORY$; = 
